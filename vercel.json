{
  "rewrites": [
    {
      "source": "/old-url",
      "destination": "/new-url"
    }
  ],
  "redirects": [
    {
      "source": "/deprecated-url",
      "destination": "/new-url",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Custom-Header",
          "value": "my-custom-header-value"
        }
      ]
    }
  ]
}
